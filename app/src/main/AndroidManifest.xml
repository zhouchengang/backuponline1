<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.zhouchengang.fileonlinelaunchapp">


    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>

    <uses-permission
        android:name="android.permission.WRITE_MEDIA_STORAGE"
        tools:ignore="ProtectedPermissions" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name="com.zhouchengang.backuponline.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/weixin"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/weixin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity
            android:name="com.zhouchengang.backuponline.album.FoneActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.zhouchengang.backuponline.album.HomeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.zhouchengang.backuponline.StartupActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.zhouchengang.backuponline.album.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"></activity>


        <activity
            android:name="com.zhouchengang.backuponline.Fone2Activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:hardwareAccelerated="true"
            android:label="@string/login" />

        <activity
            android:name="com.zhouchengang.backuponline.album.SettingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:hardwareAccelerated="true"
            android:label="@string/setting" />

        <activity
            android:name="com.zhouchengang.backuponline.album.AlbumActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/album" />

        <activity
            android:name="com.zhouchengang.backuponline.album.PicActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/album" />


        <service
            android:name="com.zhouchengang.backuponline.album.MainService"
            android:enabled="true"
            android:exported="true"
            android:priority="888" />

        <receiver
            android:name="com.zhouchengang.backuponline.base.CustomReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.zhouchengang.backuponline.MY_BROADCAST" />
            </intent-filter>
        </receiver>

    </application>

</manifest>